<properties>
<comment>Englisth(US) locale</comment>
<!-- jp/l1j/server/GameServer.java -->
<!-- run() -->
<entry key="MemoryUsage">Memory usage: %sMB</entry>
<entry key="WaittingForClient">Waiting for client connections...</entry>
<entry key="TryingToConnection">Trying to connection: %s</entry>
<entry key="BannedIP">Banned IP(%s)</entry>
<!-- printStartupMessage() -->
<entry key="GenerateServerSocket">Generate a server socket</entry>
<entry key="ServerSettings">■■■■ Server Settings ■■■■</entry>
<entry key="Exp">Exp: x%s</entry>
<entry key="Lawful">Lawful: x%s</entry>
<entry key="Karma">Karma: x%s</entry>
<entry key="ItemDrop">Item Drop: x%s</entry>
<entry key="AdenaDrop">Adena Drop: x%s</entry>
<entry key="EnchantWeapon">Enchant Weapon: %s%%</entry>
<entry key="EnchantArmor">Enchant Armor: %s%%</entry>
<entry key="EnchantAttribute">Enchant Attribute: %s%%</entry>
<entry key="GlobalChat">Global Chat: Lv%s</entry>
<entry key="PvP">PvP: OK</entry>
<entry key="NonPvP">PvP: NG(Non-PvP)</entry>
<entry key="MaxUsers">Max Users: %d users</entry>
<!-- initialize -->
<entry key="LoadingComplete">Loading Complete</entry>
<!-- ServerShutdownThread -->
<entry key="ShutdownTheServer">Shutdown the server.</entry>
<entry key="PleaseLogout">Please logout in safe area.</entry>
<entry key="ShutdownAfterFewMinutes">Server will be shutdown after %d minutes.</entry>
<entry key="ShutdownAfterFewSeconds">Server will be shutdown after %d seconds. Please logout as soon as possible.</entry>
<entry key="ShutdownAbort">Shutdown abort. The server is running normally.</entry>
</properties>
